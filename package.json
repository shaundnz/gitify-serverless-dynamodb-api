    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.438.0",
    "@aws-sdk/lib-dynamodb": "^3.438.0",
    "aws-cdk-lib": "2.101.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
